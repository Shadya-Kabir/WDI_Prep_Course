for (var i = 1; i < 101; i++) {
    if((i%3 == 0) && (i%5 == 0)){
    console.log("The number "+i+" is fizzbuzz");
    }
    else if((i%5 == 0)){
    console.log("The number "+i+" is fizz");
    }
    else if((i%3 == 0)){
    console.log("The number "+i+" is buzz");
    }
    /* else{
    console.log("The number "+i+" is not a fizz buzz");
    } */
}
